<%= form_for @user do |f| %>
  <%= render 'shared/error_messages', object: f.object %>
  <div class="col-md-8 well">
    <div class="col-md-12">
      <%= f.label "Technicals" %><br/>
      <%= f.select :technical_ids, Subject.all.collect{|s| [s.name, s.id]}, {}, {class: 'select-subjects form-control', multiple: :multiple} %>
    </div>
  </div>
  <div class="col-md-8 well">
    <%= f.submit "Submit", class: "btn btn-primary" %>
  </div>
<% end %>
